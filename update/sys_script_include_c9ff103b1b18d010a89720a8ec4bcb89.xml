        <api_name>x_snc_datasilos.IsTableSupported</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>The function returns true if the given table is supported by the DataSilos application.</description>
        <name>IsTableSupported</name>
        <script><![CDATA[function IsTableSupported(strTable)
{
	var ArrayValueExists = x_snc_devtools.ArrayValueExists;
	if (ArrayValueExists(GetAllSupportedTables(),strTable) == true)
	{
		return true;
	}
	return false;
}]]></script>
